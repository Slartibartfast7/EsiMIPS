all: decodage.o memoire.o operation.o main.o
	gcc -o main decodage.o memoire.o operation.o main.o

decodage.o: decodage.c decodage.h
	gcc -g -c decodage.c -Wall -o decodage.o

memoire.o: memoire.c memoire.h operation.h
	gcc -g -c memoire.c -Wall -o memoire.o

operation.o: operation.c operation.h
	gcc -g -c operation.c -Wall -o operation.o

main.o: main.c
	gcc -g -c main.c -Wall -o main.o

clean: 
	rm *.o